@model IEnumerable<Traveller.ServiceReference1.Posted>

@{
    ViewBag.Title = "Search";
}
@using (Html.BeginForm("Search", "Traveller", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()


    <h2>Search</h2>
    <br />

    <div class="form-group">
        <input type="text" name="searching" />
    </div>

    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" value="Search" class="btn btn-default" />
        </div>
    </div>

}





@foreach (var item in Model)
{

    <div>
        <h4>@item.Title</h4>
        <hr />
        <dl class="dl-horizontal">
            <dt>
                content
            </dt>

            <dd>
                @Html.DisplayFor(model => item.Content)
            </dd>

            <dt>
                createtDate
            </dt>

            <dd>
                @Html.DisplayFor(model => item.CreatedAt)
            </dd>


        </dl>
    </div>
    <div>

    </div>


    <p>
        @Html.ActionLink("Details", "PostDetail", new { id = item.id }) 
    </p>
    //     @Html.ActionLink("Edit", "Edit", new { id = item.id }) |
    // @Html.ActionLink("Details", "Details", new { id = item.id }) |
    //     @Html.ActionLink("Delete", "Delete", new { id = item.id })
}


